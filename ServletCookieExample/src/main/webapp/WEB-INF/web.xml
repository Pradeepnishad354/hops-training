<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
	version="4.0">
	<display-name>ServletCookieExample</display-name>


	<servlet>
		<servlet-name>first</servlet-name>
		<servlet-class>com.cookie.Servlet1</servlet-class>
		<init-param>
		<param-name>name1</param-name>
		<param-value>mahadev</param-value>
		
		</init-param>
	</servlet>
	
	<servlet>
		<servlet-name>second</servlet-name>
		<servlet-class>com.cookie.Servlet2</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>first</servlet-name>
		<url-pattern>/Servlet1</url-pattern>


	</servlet-mapping>


	<servlet-mapping>
		<servlet-name>second</servlet-name>
		<url-pattern>/Servlet2</url-pattern>


	</servlet-mapping>

<context-param>
<param-name>name1</param-name>
<param-value>pradeep</param-value>

</context-param>

<context-param>
<param-name>phone</param-name>
<param-value>realme</param-value>

</context-param>

</web-app>